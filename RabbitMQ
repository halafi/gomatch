# Gomatch config file for the RabbitMQ messaging system.
# Usage: "gomatch -a filepath_to_this_file".

amqp.receive.uri = amqp://guest:guest@localhost:5672/
amqp.receive.format = json
amqp.receive.queue = logs
amqp.receive.exchange = logs
amqp.matched.send.uri = amqp://guest:guest@localhost:5672/
amqp.matched.send.queue = gomatch


